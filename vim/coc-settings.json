{
  "snippets.extends": {
    "cpp": [
      "c"
    ]
  },
  "snippets.ultisnips.directories": [
    "~/.vim/mysnippets/"
  ],
  "suggest.noselect": false,
  "suggest.detailField": "abbr",
  "diagnostic.virtualText": true,
  "suggest.snippetIndicator": "ðŸŒŸ",
  "languageserver": {
    "clangd": {
      "command": "clangd-9",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ]
    }
  },
  "latex.build.args": [
    "-pdf",
    "-interaction=nonstopmode",
    "-synctex=1",
    "-pv"
  ],
  "latex.build.onSave": true,
  "latex.forwardSearch.executable": "okular",
  "latex.forwardSearch.args": [
    "--unique",
    "file:%p#src:%l%f"
  ],
  "python.jediEnabled": true,
  "coc.source.vimtex.enable": true,
  "coc.source.word.filetypes": [
    "tex",
    "latex",
    "text"
  ],
  "list.source.outline.ctagsFilestypes": [
    "c",
    "cpp"
  ],
  "java.errors.incompleteClasspath.severity": "ignore"
}
